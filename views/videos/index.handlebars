<div class="contents-container" id="videos-container">
{{#each videos as |video|}}
  <div class="video-card">
    <a class="video-title" href="/videos/{{video._id}}">
      {{video.title}}
    </a>
    <iframe src="{{video.url}}" frameborder="0" class="video-player"></iframe>
  </div>
{{/each}}
</div>
<a href="/videos/create" class="video-button" id="create-video">Add a video</a>
